{
  "name": "final-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node build/index.js | pino-pretty --colorize",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts | pino-pretty --colorize",
    "postinstall": "pnpx typesync",
    "prisma": "dotenv -e .env prisma",
    "db:gen": "pnpm run prisma generate",
    "db:studio": "pnpm run prisma studio",
    "db:migrate": "pnpm run prisma migrate dev",
    "db:push": "pnpm run prisma db push",
    "lint": "eslint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@fastify/helmet": "^10.1.0",
    "@prisma/client": "^4.6.1",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "env-schema": "^4.0.0",
    "fastify": "^4.10.2",
    "fastify-cli": "^5.6.0",
    "fastify-now": "^3.0.2",
    "fastify-plugin": "^4.3.0",
    "fluent-json-schema": "^3.1.0",
    "http-errors": "^2.0.0",
    "pino": "^7.10.0",
    "undici": "^5.13.0"
  },
  "devDependencies": {
    "@types/eslint": "8.4.1",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/http-errors": "1.8.2",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.25",
    "@types/nodemon": "1.19.1",
    "@types/pino": "7.0.4",
    "@types/prettier": "2.6.0",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "cross-env": "7.0.3",
    "eslint": "8.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "lint-staged": "12.3.8",
    "nodemon": "2.0.15",
    "pino-pretty": "7.6.1",
    "prettier": "2.6.2",
    "prisma": "4.6.1",
    "ts-node": "10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.6.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,ts,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run build"
    }
  }
}
